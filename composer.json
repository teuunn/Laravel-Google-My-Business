{
    "name": "teuunn/laravel-google-my-business",
    "description": "A package for Laravel which implements the Google My Business API",
    "keywords": ["google", "google my business", "laravel", "laravel8", "laravel9"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Scott Bowler",
            "email": "scott.bowler@dcsworldwide.net"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "illuminate/support": "6.* || 7.* || 8.* || 9.*",
        "google/apiclient": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Teuunn\\LaravelGoogleMyBusiness\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Teuunn\\LaravelGoogleMyBusiness\\GoogleMyBusinessServiceProvider"
            ],
            "aliases": {
                "GoogleMyBusiness": "Teuunn\\LaravelGoogleMyBusiness\\Facades\\GoogleMyBusinessFacade"
            }
        }
    }
}
